    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-windicss": "^1.6.3",
    "vitest": "^0.3.0",
    "vue": "^3.2.25",
    "vue-tsc": "^0.31.2",
    "windicss": "^3.4.3"
